<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Rect" >
        <id name="id" type="java.lang.Integer" >
            <generator class="increment"/>
        </id>
        <property name="rgb" type="java.lang.Integer"/>
        <property name="type" type="java.lang.String"/>
        <property name="x1" type="java.lang.Integer"/>
        <property name="x2" type="java.lang.Integer"/>
        <property name="y1" type="java.lang.Integer"/>
        <property name="y2" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>